                DefinitieHerkomst: RIO
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="Bedrijfsdocument">
                <xsd:sequence>
                    <xsd:element maxOccurs="1" minOccurs="1" name="requestGoedgekeurd" type="LeveringGoedgekeurd"/>
                    <xsd:element maxOccurs="99" minOccurs="0" name="foutmelding" type="Foutmelding"/>
                    <xsd:element maxOccurs="1" minOccurs="0" name="onderwijslocatiecode" type="OnderwijslocatieID-v01"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
