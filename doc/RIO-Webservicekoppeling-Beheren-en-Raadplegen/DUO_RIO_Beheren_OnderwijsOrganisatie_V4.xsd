            <xsd:element maxOccurs="2" minOccurs="2" name="opleidingseenheidcode" type="OpleidingseenheidID-v01"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="particuliereOpleiding" type="ParticuliereOpleiding" substitutionGroup="opleidingseenheid"/>
