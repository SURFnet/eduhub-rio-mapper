            <xsd:element maxOccurs="99" minOccurs="0" name="kenmerken" type="Kenmerk"/>
            <xsd:element maxOccurs="0" minOccurs="0" name="kenmerkwaardenbereik_OnderwijsaanbiedergroepPeriode" type="Kenmerkwaardenbereik_OnderwijsaanbiedergroepPeriode"/>
