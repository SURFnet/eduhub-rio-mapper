        <xsd:sequence>
            <xsd:element maxOccurs="1" minOccurs="1" name="onderwijsaanbiedergroepcode" type="OnderwijsaanbiedergroepID-v01"/>
            <xsd:element maxOccurs="1" minOccurs="1" name="inBedrijfdatum" type="InBedrijfdatum-v01"/>
            <xsd:element maxOccurs="1" minOccurs="0" name="uitBedrijfdatum" type="UitBedrijfdatum-v01"/>
            <xsd:element maxOccurs="999" minOccurs="1" name="onderwijsaanbiedergroepPeriode" type="OnderwijsaanbiedergroepPeriode"/>
