            <xsd:element maxOccurs="1" minOccurs="1" name="onderwijslocatie" type="Onderwijslocatie"/>
            <xsd:element maxOccurs="1" minOccurs="0" name="uitBedrijfdatum" type="Onderwijslocatiegebruikeinddatum-v01"/>
            <xsd:element maxOccurs="0" minOccurs="0" name="kenmerkwaardenbereik_Onderwijslocatiegebruik" type="Kenmerkwaardenbereik_Onderwijslocatiegebruik"/>
            <xsd:element maxOccurs="99" minOccurs="0" name="juridischeOnderwijslocatiegebruikRelatie" type="JuridischeOnderwijslocatiegebruikRelatie"/>
            <xsd:element maxOccurs="99" minOccurs="0" name="kenmerken" type="Kenmerk"/>
        </xsd:sequence>
    </xsd:complexType>
