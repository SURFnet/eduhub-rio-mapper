            <xsd:element maxOccurs="1" minOccurs="0" name="internationaleNaam" type="InternationaleNaam-v01"/>
            <xsd:element maxOccurs="99" minOccurs="0" name="kenmerken" type="Kenmerk"/>
            <xsd:element maxOccurs="0" minOccurs="0" name="kenmerkwaardenbereik_OnderwijsaanbiederPeriode" type="Kenmerkwaardenbereik_OnderwijsaanbiederPeriode"/>
